{
  "name": "myangelito",
  "main": "./worker/index.ts",
  "compatibility_date": "2025-04-03",
  "build": {
    "command": "npm run build"
  },
  "assets": {
    "directory": "dist/client",
    "not_found_handling": "single-page-application"
  },
  "routes": [
    {
      "pattern": "myangelito.com/*",
      "zone_name": "myangelito.com"
    }
  ],
  "observability": {
    "logs": {
      "enabled": true
    }
  },
  "vars": {
    "ENVIRONMENT": "development",
    "GOOGLE_CLIENT_ID": "",
    "APPLE_CLIENT_ID": ""
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "angelito-db",
      "database_id": "0d089ac3-bcd3-4b7b-a9c5-5fa8bba6108e",
      "migrations_dir": "./worker/db/migrations"
    }
  ]
}
